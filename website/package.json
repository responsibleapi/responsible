{
  "name": "@responsibleapi/website",
  "version": "0.0.1",
  "sideEffects": false,
  "scripts": {
    "chk": "concurrently bun:chk:*",
    "chk:lint": "eslint src/ --fix",
    "chk:test": "bun test",
    "chk:types": "tsc",
    "deps": "bunx npm-check-updates --interactive --install always",
    "dev": "vite --host",
    "dpl": "bun chk && rm -rf dist/ && vite build && wrangler pages deploy dist/ --commit-dirty=true"
  },
  "dependencies": {
    "ace-builds": "^1.33.2",
    "kdljs": "^0.2.0",
    "solid-js": "^1.8.17"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.13",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-solid": "^2.10.2",
    "wrangler": "^3.56.0"
  }
}
