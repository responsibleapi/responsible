          "videoFeedURL": {
            "type": "string",
            "format": "uri"
          },
          "website": {
            "type": "string",
            "format": "uri"
          },
          "youtubeURL": {
            "type": "string",
            "format": "uri"
          }
        }
      },
      "ItemsResp": {
        "required": ["items", "total"],
        "type": "object",
        "properties": {
          "items": {
            "type": "array",
            "items": {
              "required": [
                "audio_url",
                "id",
                "pub_date_utc",
                "title",
