{
  "openapi": "3.0.1",
  "info": {
    "title": "Listenbox",
    "version": "0.1"
  },
  "components": {
    "schemas": {
      "LoginReq": {
        "required": ["email", "host"],
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "format": "email"
          },
          "host": {
            "type": "string"
          }
        }
      },
      "OtpReq": {
        "required": ["email", "otp"],
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "format": "email"
          },
          "otp": {
            "minLength": 1,
            "type": "string"
          },
          "updates": {
            "type": "boolean"
          }
        }
      },
      "SubmitReq": {
        "required": ["url"],
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "format": "uri"
          }
        }
      },
      "UnsubscribeReq": {
        "required": ["email"],
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "format": "email"
          }
        }
      },
      "UserReq": {
        "required": ["updates"],
        "type": "object",
        "properties": {
          "updates": {
            "type": "boolean"
          }
        }
      },
      "CheckoutReq": {
        "required": ["cancel_url", "interval", "plan", "success_url"],
        "type": "object",
        "properties": {
          "cancel_url": {
            "type": "string",
            "format": "uri"
          },
          "interval": {
            "type": "string",
            "enum": ["month", "year"]
          },
          "plan": {
            "type": "string",
            "enum": ["free", "basic", "creator"]
          },
          "success_url": {
            "type": "string",
            "format": "uri"
          }
        }
      },
      "PortalReq": {
        "required": ["return_url"],
        "type": "object",
        "properties": {
          "return_url": {
            "type": "string",
            "format": "uri"
          }
        }
      },
      "EditShowReq": {
        "required": ["explicit", "language", "owner", "ownerEmail"],
        "type": "object",
        "properties": {
          "analyticsPrefix": {
            "type": "string",
            "format": "uri"
          },
          "author": {
            "type": "string"
          },
          "category1": {
            "type": "string"
          },
          "category2": {
            "type": "string"
          },
          "copyright": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "explicit": {
            "type": "boolean"
          },
          "image": {
            "type": "string",
            "format": "uri"
          },
          "keywords": {
            "type": "string"
          },
          "language": {
            "type": "string"
          },
          "owner": {
            "type": "string"
          },
          "ownerEmail": {
            "type": "string",
            "format": "email"
          },
          "subcategory1": {
            "type": "string"
          },
          "subcategory2": {
            "type": "string"
          },
          "title": {
            "type": "string"
          },
          "website": {
            "type": "string",
            "format": "uri"
          }
        }
      },
      "ReverseReq": {
        "required": ["showID", "value"],
        "type": "object",
        "properties": {
          "showID": {
            "type": "string"
          },
          "value": {
            "type": "boolean"
          }
        }
      },
      "ErrorStruct": {
        "required": ["name", "type", "value"],
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "type": {
            "type": "string",
            "enum": [
              "NO_MATCH",
              "NOT_FOUND",
              "UNEXPECTED_ARRAY",
              "UNEXPECTED_SINGLE_STRING",
              "FILE_NOT_FOUND",
              "WRONG_CONTENT_TYPE",
              "EMPTY_VALUE",
              "UNEXPECTED_ARRAY_SIZE",
              "DESERIALIZATION_ERROR",
              "OBJECT_FIELD_NOT_FOUND",
              "JSON_NOT_PARSABLE",
              "JSON_INVALID",
              "XML_INVALID"
            ]
          },
          "value": {
            "type": "string"
          }
        }
      },
      "LoginResp": {
        "required": ["login"],
        "type": "object",
        "properties": {
