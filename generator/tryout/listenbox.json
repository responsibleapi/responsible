      "Unit": {
        "type": "object",
        "additionalProperties": false
      },
      "SubmitResp": {
        "required": ["showID"],
        "type": "object",
        "properties": {
          "plan": {
            "type": "string",
            "enum": ["free", "basic", "creator", null]
          },
          "showID": {
            "required": ["id"],
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              }
            }
          }
        }
      },
      "Show": {
        "required": [
          "audioFeedURL",
          "episodes",
          "feed_id",
          "id",
          "language",
          "title",
          "videoFeedURL",
          "youtubeURL"
        ],
        "type": "object",
        "properties": {
          "analyticsPrefix": {
            "type": "string",
            "format": "uri"
          },
          "audioFeedURL": {
            "type": "string",
            "format": "uri"
          },
          "author": {
            "type": "string"
          },
          "copyright": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "episodes": {
            "type": "integer",
            "format": "int32"
          },
          "explicit": {
            "type": "boolean"
          },
          "feed_id": {
            "type": "string"
          },
          "id": {
            "type": "string"
          },
          "image": {
            "type": "string",
            "format": "uri"
          },
          "keywords": {
            "type": "string"
          },
          "language": {
            "type": "string"
          },
          "owner": {
            "type": "string"
          },
          "ownerEmail": {
            "type": "string",
            "format": "email"
          },
          "primaryCategory": {
            "required": ["category"],
            "type": "object",
            "properties": {
              "category": {
                "type": "string"
              },
              "subcategory": {
                "type": "string"
              }
            }
          },
          "refreshedUTC": {
            "type": "integer",
            "format": "int64"
          },
          "reverse": {
            "type": "boolean"
          },
